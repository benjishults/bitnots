(implies (forall ((x) (y)) 
	   (= (the-union x y) (the-class-of-all (z) 
				(or (a-member-of z x) (a-member-of z y)))))
  (forall ((z) (x) (y)) 
    (iff (a-member-of z (the-union x y))
	 (or (a-member-of z x) (a-member-of z y)))))
;;; this also shows why scripts would be useful.
